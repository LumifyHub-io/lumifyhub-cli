{
  "name": "lumifyhub-cli",
  "version": "0.1.0",
  "description": "CLI tool for LumifyHub - sync and manage your pages locally",
  "type": "module",
  "bin": {
    "lh": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --watch",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "lumifyhub",
    "cli",
    "notes",
    "sync"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "glob": "^11.0.0",
    "gray-matter": "^4.0.3",
    "ora": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
